{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.stablePeek": true,
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/*/**": true,
    "**/build/**": true
  },
  "markdownlint.config": {
    "MD041": false
  },
  "spellright.language": [
    "en_GB"
  ],
  "spellright.notificationClass": "warning",
  "cSpell.ignoreWords": [
    "Ionescu",
    "Liviu",
    "NSIS",
    "Somsubhra",
    "UCRT",
    "caffeinate",
    "cleanups",
    "gettext",
    "gnumake",
    "ilegeul",
    "liquidjs",
    "loglevel",
    "msys",
    "npmignore",
    "npmjs",
    "postversion",
    "rmyorston",
    "userdel",
    "wksi",
    "workdir",
    "xbbli",
    "xpack",
    "xpacks"
  ]
}
